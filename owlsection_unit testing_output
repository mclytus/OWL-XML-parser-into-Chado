<?php
/**
 * @files
 * @add file from OWL OBO header
 */

// Loading the XML file
$owl = new XMLReader();
// Open the OWL file for parsing.
$owl->open('ro.owl');
$section = array();

$num_ops = 0;
while ($owl->read()) {
  if ($owl->nodeType == XMLReader::ELEMENT and $owl->name == "owl:ObjectProperty") {
    print $owl->getAttribute('rdf:about') . "\n";
    $num_ops++;
    switch ($owl->name) {
      case "rdfs:seeAlso":
        for ($n = 0; $n < $owl->attributeCount; $n++) {
          $owl->moveToAttributeNo($n);
        }
        break;
      case "rdfs:subPropertyOf":
        for ($n = 0; $n < $owl->attributeCount; $n++) {
          $owl->moveToAttributeNo($n);
        }
        break;
    }
  }
}
print "Num Ops: $num_ops\n";  
